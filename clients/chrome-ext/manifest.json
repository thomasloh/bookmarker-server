{
  "name": "leafy",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Bookmarker",
  "homepage_url": "http://extensionizr.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/js/vendor/jquery.min.js",
      "src/js/vendor/jquery.cookie.js",
      "src/js/vendor/underscore.min.js",
      "src/js/vendor/backbone.min.js",
      "src/js/leafy.js",
      "src/js/background.js"
    ],
    "persistent": false
  },
  "options_page": "src/html/options.html",
  "permissions": [
    "activeTab",
    "tabs",
    "cookies",
    "http://localhost/*"
  ],
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Save to Leafy"
  }
}
